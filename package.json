{
  "name": "js-algos",
  "version": "1.0.0",
  "description": "A Repo of algorithms written in javascript and explained",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "lint": "eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty .",
    "spec": "NODE_ENV=production jest !flow-typed .",
    "test": "npm run lint && npm run spec"
  },
  "jest": {
    "testRegex": "[a-z].*\\.js$",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/flow-typed/",
      "coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/js-algorithims.git"
  },
  "author": "Amila Welihinda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amilajack/algorithims/issues"
  },
  "homepage": "https://github.com/amilajack/algorithims#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "1.1.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-flowtype-errors": "^2.0.2",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.37.4",
    "jest-cli": "^18.0.0",
    "tcomb": "^3.2.16"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  }
